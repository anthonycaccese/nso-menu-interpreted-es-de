         </systemstatus>
      </view>
      <fontSize name="medium">
         <variables>
            <avatarMaskSize>0.069444444444444 0.111111111111111</avatarMaskSize><!-- 80 80 -->
            <avatarSize>0.03472222 0.05555556</avatarSize><!-- 40 40 -->
