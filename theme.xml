      </fontSize>
   </aspectRatio>

   <!--
   ///
   Aspect Ratio: 4:3
   ///
   -->
   <aspectRatio name="4:3">
      <view name="system,gamelist">
         <image name="avatar-mask,avatar-border,avatar-border-shadow">
            <size>${avatarMaskSize}</size>
         </image>
         <image name="avatar-image">
            <cropSize>${avatarSize}</cropSize>
         </image>
      </view>
      <view name="system">
         <image name="avatar-image">
            <pos>${avatarSystemPos}</pos>
         </image>
      </view>
      <view name="gamelist">
         <image name="avatar-image">
            <pos>${avatarGamelistPos}</pos>
         </image>
      </view>
      <view name="system">
         <text name="menu-title-system">
            <pos>${menuBarSystemTitlePos}</pos>
